
@{
    ViewBag.Title = "OrderDetails";
}

<h2>OrderDetails</h2>


<table class="table table-hover">
    <tr>
        <th>Name </th>
        <th>Price </th>
        <th>Description</th>
        <th>Date</th>

    </tr>

    <tr>
        @foreach (var item in Model)
        {

    <tr>
        <td>@item.Name</td>
        <td>@item.Price</td>
        <td>@item.Description</td>
        <td>@item.Date</td>
     


    </tr>
    }
        </tr>

    </table>
